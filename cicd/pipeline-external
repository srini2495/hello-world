pipeline {
  agent any
  options {
    disableConcurrentBuilds()
    disableResume()
    buildDiscarder(logRotator(numToKeepStr: '10'))
    timeout(time: 1, unit: 'HOURS')
  }
  tools {
    maven 'maven-3.6.3' 
  }
  stages {
    stage ('Connect to vm') {
      steps {
        sh 'ssh -v -i /home/srini/.ssh/srini-ubuntu_key.pem azureuser@4.240.36.178'
        sh 'hostname'
      }
    }
  }
}
